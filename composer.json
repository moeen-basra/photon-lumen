{
  "name": "moeen/photon-lumen",
  "description": "Lumen Extension for SRP (Single Responsibility Principle)",
  "keywords": [
    "photon",
    "laravel",
    "lumen",
    "laravel-lumen"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Moeen Basra",
      "email": "m.basra@live.com"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "ext-json": "*"
  },
  "require-dev": {
    "laravel/lumen-framework": "^8.1"
  },
  "autoload": {
    "psr-4": {
      "Photon\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
